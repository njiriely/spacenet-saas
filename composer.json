
{
    "name": "spacenet/saas-wifi-billing",
    "description": "SaaS WiFi Billing System for Internet Service Providers in Kenya",
    "type": "project",
    "license": "MIT",
    "keywords": ["wifi", "billing", "saas", "mikrotik", "mpesa", "kenya"],
    "authors": [
        {
            "name": "SPACE NET Team",
            "email": "hello@spacenet.co.ke"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "SpaceNet\\": "includes/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
            "php install/setup.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
